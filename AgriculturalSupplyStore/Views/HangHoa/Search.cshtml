@model IEnumerable<AgriculturalSupplyStore.ViewModels.MenuHangHoaVM>
@{
	ViewData["Title"] = @ViewBag.NameSearch;
}
<!-- Header Start -->
<div class="container-fluid bg-breadcrumb">
	<div class="container text-center py-5" style="max-width: 900px;">
		<h4 class="text-white display-4 mb-4 wow fadeInDown " data-wow-delay="0.1s">Tìm Kiếm</h4>
		<ol class="breadcrumb d-flex justify-content-center mb-0 wow fadeInDown" data-wow-delay="0.3s">
			<li class="breadcrumb-items_first "><a>Trang Chủ</a></li>
			<li class="breadcrumb-items active text-primary"><a>Tìm Kiếm</a></li>
			<li class="breadcrumb-items active text-primary"><a>@ViewBag.NameSearch</a></li>
		</ol>
	</div>
</div>
<!-- Header End -->
@await Html.PartialAsync("ProductItem", Model)