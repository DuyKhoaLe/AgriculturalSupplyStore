@model IEnumerable<AgriculturalSupplyStore.ViewModels.MenuHangHoaVM>

<div class="">
    <table class="table table-striped ">
        <thead class="">
            <tr>
                <th>STT</th>        
                <th>Tên hàng hóa</th>
                <th>Mã số</th>
                <th>Đơn giá</th>
                <th>Đặt hàng</th>
            </tr>
        </thead>
        <tbody>
            @{
                int stt = 1;
            }
            @foreach (var item in Model)
            {
                <tr>
                    <td>@stt</td>                 
                    <td>@item.TenHh</td>
                     <td>@item.MaHh</td>
                    <td>@item.DonGia</td>
                    <td></td>
                </tr>
                stt++;
            }
        </tbody>
    </table>
</div>

